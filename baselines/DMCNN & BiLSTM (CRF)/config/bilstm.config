[train]
epoch = 15
batch_size = 200
shuffle = True
valid_interval = 1
save_strategy = save_best

[test]
batch_size = 200
shuffle = False

[data]
reader_name = MavenReader
formatter_name = BilstmFormatter
word2vec_file = 100.utf8
split_labels = True

[model]
model_name = Bilstm
num_layers = 1
hidden_size = 256
dropout = 0.5

[optimizer]
optimizer_name = Adam
lr = 1e-3
weight_decay = 1e-8